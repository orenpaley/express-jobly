{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "title": "Root Schema",
  "type": "object",
  "default": {},
  "required": [
      "title",
      "salary",
      "equity",
      "companyHandle"
  ],
  "properties": {
      "id": {
          "title": "The id Schema",
          "type": "integer",
          "default": 0,
          "examples": [
              1
          ]
      },
      "title": {
          "title": "The title Schema",
          "type": "string",
          "default": "",
          "examples": [
              "new"
          ]
      },
      "salary": {
          "title": "The salary Schema",
          "type": "integer",
          "default": 0,
          "examples": [
              1000
          ]
      },
      "equity": {
          "title": "The equity Schema",
          "type": "string",
          "default": "",
          "examples": [
              "0,20"
          ]
      },
      "company_handle": {
          "title": "The company_handle Schema",
          "type": "string",
          "default": "",
          "examples": [
              "c1"
          ]
      }
  },
  "examples": [{
      "id": 1,
      "title": "new",
      "salary": 1000,
      "equity": "0,20",
      "company_handle": "c1"
  }]
}